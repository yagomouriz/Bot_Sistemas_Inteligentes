<?xml version="1.0" encoding="UTF-8"?>
<aiml>

<!--primera regla-->
<category>
<pattern>Hola *</pattern>
<template>Hola, ¿Qué tal?</template>
</category>

<!--segunda regla-->
<category>
<pattern> ^ que tal y tu</pattern>
<template>Estoy genial!</template>
</category>

<!--tercera regla-->
<category>
<pattern> ^ me llamo *</pattern>
<template>Que casualidad <star index="2"/>, yo me llamo: oscar</template>
</category>

</aiml>
